/// Audit Trail Logs - Comprehensive activity logging for enterprise compliance
model audit_trail_logs {
  id              String   @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  user_id         String?  @db.Uuid
  action          String
  entity_type     String
  entity_id       String?  @db.Uuid
  changes         Json?
  previous_values Json?
  ip_address      String?
  user_agent      String?
  status          String   @default("success")
  error_message   String?
  created_at      DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  users           public_users? @relation(fields: [user_id], references: [id], onDelete: SetNull, onUpdate: NoAction)

  @@index([user_id])
  @@index([entity_type, entity_id])
  @@index([action])
  @@index([created_at(sort: Desc)])
  @@map("audit_trail_logs")
  @@schema("public")
}

/// Onboarding Steps Definition - Role-specific onboarding workflow
model onboarding_steps {
  id                        String   @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  step_key                  String   @unique
  role                      String
  step_number               Int
  step_name                 String
  description               String?
  is_required               Boolean  @default(true)
  created_at                DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  onboarding_completed      onboarding_steps_completed[]

  @@unique([role, step_number])
  @@index([role, step_number])
  @@map("onboarding_steps")
  @@schema("public")
}

/// Onboarding Progress - Tracks individual user onboarding journey
model onboarding_progress {
  id                    String   @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  user_id               String   @unique @db.Uuid
  role                  String
  current_step          Int      @default(1)
  total_steps           Int
  completed_percentage  Int      @default(0)
  started_at            DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  completed_at          DateTime? @db.Timestamptz(6)
  is_completed          Boolean  @default(false)
  created_at            DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  updated_at            DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  users                 public_users @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([user_id])
  @@index([role])
  @@index([is_completed])
  @@index([completed_at])
  @@map("onboarding_progress")
  @@schema("public")
}

/// Onboarding Steps Completed - Tracks which steps user has completed
model onboarding_steps_completed {
  id                  String   @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  user_id             String   @db.Uuid
  step_id             String   @db.Uuid
  step_key            String
  completed_at        DateTime @default(dbgenerated("timezone('utc'::text, now())")) @db.Timestamptz(6)
  completed_by_action String?
  metadata            Json?
  users               public_users @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  onboarding_steps    onboarding_steps @relation(fields: [step_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@unique([user_id, step_id])
  @@index([user_id])
  @@index([step_id])
  @@index([completed_at(sort: Desc)])
  @@map("onboarding_steps_completed")
  @@schema("public")
}

